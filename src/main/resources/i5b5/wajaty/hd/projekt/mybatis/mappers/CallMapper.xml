<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="i5b5.wajaty.hd.projekt.mybatis.mappers.CallMapper">
    <select id="getAllProvinces" resultType="i5b5.wajaty.hd.projekt.model.sources.localisation.Province">
        select *
        from PROVINCE
    </select>
    <select id="getAllDistricts" resultType="i5b5.wajaty.hd.projekt.model.sources.localisation.District">
        select *
        from DISTRICT
    </select>
    <select id="getAllLocalities" resultType="i5b5.wajaty.hd.projekt.model.sources.localisation.Locality">
        select *
        from LOCALITY
    </select>
    <select id="getAllSubscriptionTypes"
            resultType="i5b5.wajaty.hd.projekt.model.sources.entities.SubscriptionType">
        select *
        from SUBSCRIPTION_TYPE
    </select>
    <select id="getAllClients" resultType="i5b5.wajaty.hd.projekt.model.sources.entities.Client">
        select *
        from CLIENT
    </select>
    <select id="getAllCallPrices" resultType="i5b5.wajaty.hd.projekt.model.sources.call.CallPrice">
        select *
        from CALL_PRICE_LIST
    </select>
    <select id="getAllCalls" resultType="i5b5.wajaty.hd.projekt.model.sources.call.Call">
        select *
        from `CALL`
    </select>

</mapper>